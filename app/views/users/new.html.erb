<% if flash[:error] %>
<div class='row'>
  <div class = 'col-md-4'></div>
    <div id = "error" class = "col-md-4 alert alert-dismissable alert-warning">
      <button type = "button" class = "close" data-dismiss = "alert">x
      </button>
      <strong><%= flash[:error] %></strong>
    </div>
  <div class = 'col-md-4'></div>
</div>
<% end %>
<div class='row'>
  <div class = "well col-md-4 col-md-offset-4 bs-example horizontal-form">
    <fieldset>
      <%= render 'form' %>
    </fieldset>
  </div>
</div>
