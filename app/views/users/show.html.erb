<% if notice %>
<div class="row">
  <div class = 'col-md-4'></div>
  <div id = "notice" class = "col-md-4 alert alert-dismissable alert-success">
    <button type = "button" class = "close" data-dismiss = "alert">x
    </button>
    <strong><%= notice %></strong>
  </div>
  <div class = 'col-md-4'></div>
</div>
<% end %>

<div class="row">
  <h1 class = "col-md-12">My Dashboard</h1>

  <div class = "well col-md-4 col-md-offset-1">
    <%= render partial:'orders/index.html.erb', locals:{id: params[:id]} %>
  </div>

  <div class = "well col-md-4 col-md-offset-2">
    <%= render 'profile' %>
  </div>
  
</div>