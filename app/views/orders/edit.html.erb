<% if notice %>
  <div class = 'col-md-5'></div>
  <div id = "notice" class = "col-md-2 alert alert-dismissable alert-success">
    <button type = "button" class = "close" data-dismiss = "alert">x
    </button>
    <strong><%= notice %></strong>
  </div>
  <div class = 'col-md-5'></div>
<% end %>
<div class = "well col-md-4 col-md-offset-4">
  <fieldset>
    <legend>Edit "<%= @order.name %>"</legend>
    <%= render 'form' %>

  </fieldset>
</div>