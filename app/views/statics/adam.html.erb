<h1>About Chez Adam</h1>


<div id = "carousel-example-generic" class = "carousel slide col-md-5" data-ride = "carousel">
  <ol class = "carousel-indicators">
    <% for i in 1..5 %>
      <li data-target = "#carousel-example-generic" data-slide-to= <%= "#{i-1}" %>></li>
    <% end %>
  </ol>
  <div class = "carousel-inner">
    
      <div class = "item active" >
        <%= image_tag "http://storage.googleapis.com/emergenchefvideo/food/1.jpg" %>
      </div>
      <div class = "item" >
        <%= image_tag "http://storage.googleapis.com/emergenchefvideo/food/2.jpg" %>
      </div>
      <div class = "item" >
        <%= image_tag "http://storage.googleapis.com/emergenchefvideo/food/4.jpg" %>
      </div>
      <div class = "item" >
        <%= image_tag "http://storage.googleapis.com/emergenchefvideo/food/6.jpg" %>
      </div>
      <div class = "item" >
        <%= image_tag "http://storage.googleapis.com/emergenchefvideo/food/13.jpg" %>
      </div>
    
    
  </div>
  <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
  </a>
  <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
  </a>
</div>
<div class = "col-md-3"><%= image_tag "http://storage.googleapis.com/emergenchefvideo/food/me.jpg" %>
</div>